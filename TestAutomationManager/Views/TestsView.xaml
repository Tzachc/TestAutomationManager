<UserControl x:Class="TestAutomationManager.Views.TestsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestAutomationManager.Views"
             xmlns:converters="clr-namespace:TestAutomationManager.Converters"
             xmlns:scm="clr-namespace:System.ComponentModel;assembly=WindowsBase"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="1600"
             Background="{DynamicResource PrimaryBackgroundBrush}"
             Loaded="TestsView_Loaded"
             Unloaded="TestsView_Unloaded">

    <UserControl.Resources>
        <!-- Converters -->
        <converters:RunStatusToColorConverter x:Key="RunStatusToColorConverter"/>
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

        <!-- Expand Toggle -->
        <Style x:Key="ExpandToggle" TargetType="ToggleButton">
            <Setter Property="Width" Value="24"/>
            <Setter Property="Height" Value="24"/>
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="Cursor" Value="Hand"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ToggleButton">
                        <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="4">
                            <TextBlock x:Name="Arrow"
                                       Text="+" 
                                       Foreground="{DynamicResource TextTertiaryBrush}" 
                                       FontSize="25"
                                       HorizontalAlignment="Center" 
                                       VerticalAlignment="Center"
                                       RenderTransformOrigin="0.5,0.5">
                                <TextBlock.RenderTransform>
                                    <RotateTransform Angle="0"/>
                                </TextBlock.RenderTransform>
                            </TextBlock>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsChecked" Value="True">
                                <Trigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <DoubleAnimation Storyboard.TargetName="Arrow" 
                                                             Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)"
                                                             To="90" Duration="0:0:0.15"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.EnterActions>
                                <Trigger.ExitActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <DoubleAnimation Storyboard.TargetName="Arrow" 
                                                             Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)"
                                                             To="0" Duration="0:0:0.15"/>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.ExitActions>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="border" Property="Background" Value="{DynamicResource HoverBackgroundBrush}"/>
                                <Setter TargetName="Arrow" Property="Foreground" Value="{DynamicResource TextSecondaryBrush}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!-- ScrollBar -->
        <Style x:Key="ModernScrollBar" TargetType="{x:Type ScrollBar}">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="BorderBrush" Value="Transparent"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                        <Grid>
                            <Border Background="{DynamicResource SecondaryBackgroundBrush}" CornerRadius="4" Margin="4">
                                <Track x:Name="PART_Track"
                                       Orientation="{TemplateBinding Orientation}"
                                       Minimum="{TemplateBinding Minimum}"
                                       Maximum="{TemplateBinding Maximum}"
                                       Value="{TemplateBinding Value}"
                                       ViewportSize="{TemplateBinding ViewportSize}">
                                    <Track.Thumb>
                                        <Thumb>
                                            <Thumb.Style>
                                                <Style TargetType="{x:Type Thumb}">
                                                    <Setter Property="MinWidth" Value="14"/>
                                                    <Setter Property="MinHeight" Value="14"/>
                                                    <Setter Property="Background" Value="{DynamicResource BorderBrush}"/>
                                                    <Setter Property="BorderBrush" Value="Transparent"/>
                                                    <Setter Property="Template">
                                                        <Setter.Value>
                                                            <ControlTemplate TargetType="{x:Type Thumb}">
                                                                <Border Background="{TemplateBinding Background}"
                                                                        BorderBrush="{TemplateBinding BorderBrush}"
                                                                        BorderThickness="0"
                                                                        CornerRadius="4"/>
                                                            </ControlTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                    <Style.Triggers>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="Background" Value="{DynamicResource TextSecondaryBrush}"/>
                                                        </Trigger>
                                                        <Trigger Property="IsDragging" Value="True">
                                                            <Setter Property="Background" Value="{DynamicResource TextPrimaryBrush}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Thumb.Style>
                                        </Thumb>
                                    </Track.Thumb>
                                </Track>
                            </Border>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="Orientation" Value="Vertical">
                                <Setter TargetName="PART_Track" Property="IsDirectionReversed" Value="True"/>
                                <Setter Property="Width" Value="14"/>
                            </Trigger>
                            <Trigger Property="Orientation" Value="Horizontal">
                                <Setter TargetName="PART_Track" Property="IsDirectionReversed" Value="False"/>
                                <Setter Property="Height" Value="14"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!-- ScrollViewer templates -->
        <Style x:Key="ModernScrollViewerVerticalOnly" TargetType="{x:Type ScrollViewer}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollViewer}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ScrollContentPresenter Grid.Column="0"/>
                            <ScrollBar x:Name="PART_VerticalScrollBar"
                                       Style="{StaticResource ModernScrollBar}"
                                       Orientation="Vertical"
                                       Grid.Column="1"
                                       Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}"
                                       Value="{TemplateBinding VerticalOffset}"
                                       Minimum="0"
                                       Maximum="{TemplateBinding ScrollableHeight}"
                                       ViewportSize="{TemplateBinding ViewportHeight}"
                                       Margin="4,0,0,0"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="ModernScrollViewer" TargetType="{x:Type ScrollViewer}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollViewer}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <ScrollContentPresenter Grid.Column="0" Grid.Row="0"/>

                            <ScrollBar x:Name="PART_HorizontalScrollBar"
                                       Style="{StaticResource ModernScrollBar}"
                                       Orientation="Horizontal"
                                       Grid.Row="1"
                                       Grid.Column="0"
                                       Visibility="{TemplateBinding ComputedHorizontalScrollBarVisibility}"
                                       Value="{TemplateBinding HorizontalOffset}"
                                       Minimum="0"
                                       Maximum="{TemplateBinding ScrollableWidth}"
                                       ViewportSize="{TemplateBinding ViewportWidth}"
                                       Margin="0,4,0,0"/>

                            <ScrollBar x:Name="PART_VerticalScrollBar"
                                       Style="{StaticResource ModernScrollBar}"
                                       Orientation="Vertical"
                                       Grid.Row="0"
                                       Grid.Column="1"
                                       Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}"
                                       Value="{TemplateBinding VerticalOffset}"
                                       Minimum="0"
                                       Maximum="{TemplateBinding ScrollableHeight}"
                                       ViewportSize="{TemplateBinding ViewportHeight}"
                                       Margin="4,0,0,0"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>

    <!-- Layout -->
    <Grid Grid.IsSharedSizeScope="True" Margin="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <!-- Sticky main header -->
        <Border Grid.Row="0"
                Background="{DynamicResource SecondaryBackgroundBrush}"
                BorderThickness="0,0,0,1"
                BorderBrush="{DynamicResource BorderBrush}">
            <ScrollViewer x:Name="HeaderScrollViewer"
                          HorizontalScrollBarVisibility="Hidden"
                          VerticalScrollBarVisibility="Disabled">
                <Border Padding="38,8">
                    <Grid MinWidth="2000">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="32"  SharedSizeGroup="cActive"/>
                            <ColumnDefinition Width="16"  SharedSizeGroup="cStatusDot"/>
                            <ColumnDefinition Width="20"  SharedSizeGroup="cExpand"/>
                            <ColumnDefinition Width="50"  SharedSizeGroup="cTestID"/>
                            <ColumnDefinition Width="600" SharedSizeGroup="cTestName"/>
                            <ColumnDefinition Width="130" SharedSizeGroup="cLastRunning"/>
                            <ColumnDefinition Width="130" SharedSizeGroup="cLastTimePass"/>
                            <ColumnDefinition Width="150" SharedSizeGroup="cRunStatus"/>
                            <ColumnDefinition Width="100" SharedSizeGroup="cBugs"/>
                            <ColumnDefinition Width="700" SharedSizeGroup="cRecipients"/>
                            <ColumnDefinition Width="300" SharedSizeGroup="cExceptionMessage"/>
                            <ColumnDefinition Width="120" SharedSizeGroup="cSendEmailReport"/>
                            <ColumnDefinition Width="120" SharedSizeGroup="cExitOnFailure"/>
                            <ColumnDefinition Width="130" SharedSizeGroup="cRunAgainTimes"/>
                            <ColumnDefinition Width="160" SharedSizeGroup="cSnapshotMultiple"/>
                            <ColumnDefinition Width="140" SharedSizeGroup="cEmailOnFailOnly"/>
                            <ColumnDefinition Width="130" SharedSizeGroup="cDisableKillDriver"/>
                            <ColumnDefinition Width="160" SharedSizeGroup="cActions"/>
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Column="0"  Text="✓" FontSize="14" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center" ToolTip="Active"/>
                        <TextBlock Grid.Column="3"  Text="TEST ID" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="4"  Text="TEST NAME" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="5"  Text="LAST RUNNING" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="6"  Text="LAST TIME PASSED" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="7"  Text="STATUS" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="8"  Text="BUGS" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="9"  Text="RECIPIENTS EMAIL" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="10" Text="EXCEPTION MESSAGE" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="11" Text="SEND EMAIL" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="12" Text="EXIT ON FAILURE" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="13" Text="RUN AGAIN TIMES" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="14" Text="SNAPSHOT MULTIPLE" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="15" Text="EMAIL ON FAIL ONLY" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="16" Text="DISABLE KILL DRIVER" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                        <TextBlock Grid.Column="17" Text="ACTIONS" FontSize="12" FontWeight="Bold"
                                   Foreground="{DynamicResource TextPrimaryBrush}" HorizontalAlignment="Center" TextAlignment="Center"/>
                    </Grid>
                </Border>
            </ScrollViewer>
        </Border>

        <!-- Scrollable body -->
        <ScrollViewer Grid.Row="1"
                      x:Name="MainScrollViewer"
                      Margin="0"
                      HorizontalScrollBarVisibility="Auto"
                      VerticalScrollBarVisibility="Auto"
                      ScrollChanged="MainScrollViewer_ScrollChanged"
                      PreviewMouseDown="MainScrollViewer_PreviewMouseDown"
                      PreviewMouseMove="MainScrollViewer_PreviewMouseMove"
                      PreviewMouseUp="MainScrollViewer_PreviewMouseUp"
                      MouseLeave="MainScrollViewer_MouseLeave"
                      Style="{StaticResource ModernScrollViewer}"
                      FlowDirection="LeftToRight">
            <ItemsControl x:Name="TestsItemsControl" Margin="10,5,24,24">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Border Background="{DynamicResource CardBackgroundBrush}"
                                BorderBrush="{DynamicResource BorderBrush}"
                                BorderThickness="1"
                                CornerRadius="8"
                                Margin="0,0,0,6"
                                Padding="0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>

                                <!-- Test Row -->
                                <Border Grid.Row="0" Padding="24,5" Background="Transparent" CornerRadius="8,8,0,0">
                                    <Grid MinWidth="2000">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cActive"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cStatusDot"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cExpand"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cTestID"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cTestName"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cLastRunning"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cLastTimePass"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cRunStatus"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cBugs"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cRecipients"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cExceptionMessage"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cSendEmailReport"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cExitOnFailure"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cRunAgainTimes"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cSnapshotMultiple"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cEmailOnFailOnly"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cDisableKillDriver"/>
                                            <ColumnDefinition Width="Auto" SharedSizeGroup="cActions"/>
                                        </Grid.ColumnDefinitions>

                                        <CheckBox Grid.Column="0"
                                                  IsChecked="{Binding IsActive, Mode=TwoWay}"
                                                  VerticalAlignment="Center"
                                                  HorizontalAlignment="Center"/>

                                        <Ellipse Grid.Column="1" Width="10" Height="10"
                                                 Fill="{Binding RunStatus, Converter={StaticResource RunStatusToColorConverter}}"
                                                 VerticalAlignment="Center"
                                                 HorizontalAlignment="Center"
                                                 Margin="0,0,6,0"
                                                 ToolTip="{Binding RunStatus}"/>

                                        <ToggleButton Grid.Column="2" 
                                                      Style="{StaticResource ExpandToggle}"
                                                      IsChecked="{Binding IsExpanded, Mode=TwoWay}"
                                                      VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"
                                                      Margin="0,0,-10,4"/>

                                        <TextBlock Grid.Column="3" Text="{Binding TestID}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"/>

                                        <TextBlock Grid.Column="4" Text="{Binding TestName}" 
                                                   Foreground="{DynamicResource TextPrimaryBrush}" 
                                                   FontSize="13" FontWeight="SemiBold"
                                                   VerticalAlignment="Center" HorizontalAlignment="Left"
                                                   TextAlignment="Left" TextWrapping="Wrap"
                                                   ToolTip="{Binding TestName}"/>

                                        <TextBlock Grid.Column="5" Text="{Binding LastRunning}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" TextWrapping="Wrap"
                                                   ToolTip="{Binding LastRunning}"/>

                                        <TextBlock Grid.Column="6" Text="{Binding LastTimePass}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextWrapping="Wrap" TextTrimming="CharacterEllipsis"
                                                   ToolTip="{Binding LastTimePass}"/>

                                        <Border Grid.Column="7"
                                                Background="{DynamicResource SecondaryBackgroundBrush}"
                                                BorderBrush="{DynamicResource BorderBrush}"
                                                BorderThickness="1"
                                                CornerRadius="4"
                                                Padding="8,4"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                            <TextBlock Text="{Binding RunStatus}" 
                                                       Foreground="{DynamicResource TextPrimaryBrush}" 
                                                       FontSize="11" FontWeight="SemiBold"
                                                       HorizontalAlignment="Center"
                                                       TextAlignment="Center"/>
                                        </Border>

                                        <TextBlock Grid.Column="8" Text="{Binding Bugs}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding Bugs}"/>

                                        <TextBlock Grid.Column="9" Text="{Binding RecipientsEmailsList}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" TextWrapping="Wrap"
                                                   VerticalAlignment="Center" HorizontalAlignment="Center"
                                                   TextAlignment="Center" TextTrimming="CharacterEllipsis"
                                                   ToolTip="{Binding RecipientsEmailsList}"/>

                                        <TextBlock Grid.Column="10" Text="{Binding ExceptionMessage}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding ExceptionMessage}"/>

                                        <TextBlock Grid.Column="11" Text="{Binding SendEmailReport}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding SendEmailReport}"/>

                                        <TextBlock Grid.Column="12" Text="{Binding ExitTestOnFailure}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding ExitTestOnFailure}"/>

                                        <TextBlock Grid.Column="13" Text="{Binding TestRunAgainTimes}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding TestRunAgainTimes}"/>

                                        <TextBlock Grid.Column="14" Text="{Binding SnapshotMultipleFailure}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding SnapshotMultipleFailure}"/>

                                        <TextBlock Grid.Column="15" Text="{Binding EmailOnFailureOnly}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding EmailOnFailureOnly}"/>

                                        <TextBlock Grid.Column="16" Text="{Binding DisableKillDriver}" 
                                                   Foreground="{DynamicResource TextSecondaryBrush}" 
                                                   FontSize="12" VerticalAlignment="Center"
                                                   HorizontalAlignment="Center" TextAlignment="Center"
                                                   TextTrimming="CharacterEllipsis" ToolTip="{Binding DisableKillDriver}"/>

                                        <StackPanel Grid.Column="17" Orientation="Horizontal"
                                                    VerticalAlignment="Center" HorizontalAlignment="Center">
                                            <Button Style="{StaticResource IconButton}" Width="28" Height="28"
                                                    ToolTip="Edit" Tag="{Binding}" Click="EditTest_Click" Margin="2,0">
                                                <TextBlock Text="✏️" FontSize="12"/>
                                            </Button>
                                            <Button Style="{StaticResource IconButton}" Width="28" Height="28"
                                                    ToolTip="Run Test" Tag="{Binding}" Click="RunTest_Click" Margin="2,0">
                                                <TextBlock Text="▶️" FontSize="12"/>
                                            </Button>
                                            <Button Style="{StaticResource IconButton}" Width="28" Height="28"
                                                    ToolTip="Delete" Tag="{Binding}" Click="DeleteTest_Click" Margin="2,0">
                                                <TextBlock Text="🗑️" FontSize="12"/>
                                            </Button>
                                        </StackPanel>
                                    </Grid>
                                </Border>

                                <!-- PROCESSES -->
                                <Border Grid.Row="1" Padding="24,0,24,16"
                                        Visibility="{Binding IsExpanded, Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <Grid Grid.IsSharedSizeScope="True">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>

                                        <!-- Sticky Process Header -->
                                        <Border Grid.Row="0"
                                                Background="{DynamicResource SecondaryBackgroundBrush}"
                                                BorderBrush="{DynamicResource BorderBrush}"
                                                BorderThickness="0,0,0,1"
                                                Padding="20,8,16,8"
                                                Margin="42,0,0,6">
                                            <Grid Grid.IsSharedSizeScope="True" MinWidth="6600">
                                                <Grid.ColumnDefinitions>
                                                    <!-- order: expand, position, web3 operator, process name, process id, pass/fail, last running, params... -->
                                                    <ColumnDefinition Width="36"  SharedSizeGroup="procExpand"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procPosition"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procWeb3Operator"/>
                                                    <ColumnDefinition Width="700" SharedSizeGroup="procName"/>
                                                    <ColumnDefinition Width="140" SharedSizeGroup="procProcessId"/>
                                                    <ColumnDefinition Width="180" SharedSizeGroup="procPassFail"/>
                                                    <ColumnDefinition Width="180" SharedSizeGroup="procLastRunning"/>

                                                    <!-- params -->
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam01"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam02"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam03"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam04"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam05"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam06"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam07"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam08"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam09"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam10"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam11"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam12"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam13"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam14"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam15"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam16"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam17"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam18"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam19"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam20"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam21"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam22"/>
                                                    <ColumnDefinition Width="300" SharedSizeGroup="procParam23"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam24"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam25"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam26"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam27"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam28"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam29"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam30"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam31"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam32"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam33"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam34"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam35"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam36"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam37"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam38"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam39"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam40"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam41"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam42"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam43"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam44"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam45"/>
                                                    <ColumnDefinition Width="120" SharedSizeGroup="procParam46"/>
                                                </Grid.ColumnDefinitions>

                                                <TextBlock Grid.Column="0" Text="" FontSize="11" FontWeight="Bold"
                                                           HorizontalAlignment="Center" TextAlignment="Center"/>
                                                <TextBlock Grid.Column="1" Text="POSITION" FontSize="11" FontWeight="Bold"
                                                           Foreground="{DynamicResource TextPrimaryBrush}"
                                                           HorizontalAlignment="Center" TextAlignment="Center"/>
                                                <TextBlock Grid.Column="2" Text="WEB3 OPERATOR" FontSize="11" FontWeight="Bold"
                                                           Foreground="{DynamicResource TextPrimaryBrush}"
                                                           HorizontalAlignment="Center" TextAlignment="Center"/>
                                                <TextBlock Grid.Column="3" Text="PROCESS NAME" FontSize="11" FontWeight="Bold"
                                                           Foreground="{DynamicResource TextPrimaryBrush}"
                                                           HorizontalAlignment="Center" TextAlignment="Center"/>
                                                <TextBlock Grid.Column="4" Text="PROCESS ID" FontSize="11" FontWeight="Bold"
                                                           Foreground="{DynamicResource TextPrimaryBrush}"
                                                           HorizontalAlignment="Center" TextAlignment="Center"/>
                                                <TextBlock Grid.Column="5" Text="PASS/FAIL" FontSize="11" FontWeight="Bold"
                                                           Foreground="{DynamicResource TextPrimaryBrush}"
                                                           HorizontalAlignment="Center" TextAlignment="Center"/>
                                                <TextBlock Grid.Column="6" Text="LAST RUNNING" FontSize="11" FontWeight="Bold"
                                                           Foreground="{DynamicResource TextPrimaryBrush}"
